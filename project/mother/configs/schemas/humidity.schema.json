{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "SmartDepartment: Humidity Measurement",
  "description": "schema for humidity measurement",
  "type": "object",
  "properties": {
    "ts": {
      "description": "measurement timestamp",
      "type": "integer"
    },
    "humidity": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "battery": {
      "type": "integer",
      "minimum": 0,
      "maximum": 100
    }
  },
  "required": [
    "ts",
    "humidity"
  ]
}
